def inOrder(root, elements):
    if not root:
        return None
    
    inOrder(root.left, elements)
    
    elements.append(root.data)
    
    inOrder(root.right, elements)

def check_binary_search_tree_(root):
    elements = []
    
    inOrder(root, elements)
    #Inorder traversal of elements must be in ascending order and there should 
    #no duplicates
    if elements == [] + sorted(elements) and len(elements) == len(set(elements)):
        return True
    else:
        return False
