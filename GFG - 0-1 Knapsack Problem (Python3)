#W - total weight/capacity allowed, wt - weights array, val - values array

def knapSack(W, wt, val, n):
        dp = [0] + [0]*W
        
        for w in range(n):
            if wt[w] > W: continue
            for i in range(W, wt[w]-1, -1):
                dp[i] = max(dp[i], val[w] + dp[i-wt[w]])
                
        return dp[W]
