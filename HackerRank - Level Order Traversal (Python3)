def getHeight(root):
    if not root:
        return 0
    
    return max(1 + getHeight(root.right), 1 + getHeight(root.left))


def getElement(root, level):
    if not root:
        return None
    
    if level == 1:
        print(str(root), end=" ")
    
    getElement(root.left, level-1)
    getElement(root.right, level-1)
    
    
    
    
def levelOrder(root):
    #Write your code here
    level = getHeight(root)
    for l in range(1, level+1):
        getElement(root, l)
